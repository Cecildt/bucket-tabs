{
  "manifest_version": 3,
  "name": "Bucket Tabs",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "assets/images/bucket16.png",
    "32": "assets/images/bucket32.png",
    "48": "assets/images/bucket48.png",
    "128": "assets/images/bucket120.png"
  }
}
